<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.module_activity.ActivityTestC">

        <TextView
            android:id="@+id/module_activity_test_c_tv_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@id/module_activity_test_c_btn_start"
            app:layout_constraintEnd_toEndOf="@id/module_activity_test_c_btn_start"
            app:layout_constraintStart_toStartOf="@id/module_activity_test_c_btn_start" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/module_activity_test_c_btn_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="c: start b"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>